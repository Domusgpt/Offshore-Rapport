{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    // Less strict rules for this project
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-irregular-whitespace": "warn"
  },
  "globals": {
    // Add any global variables that ESLint should recognize
    "OffshoreRapportData": "readonly",
    "OffshoreRapportRenderer": "readonly"
  }
}